{
  "private": true,
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "lint": "eslint . --ext .js,.svelte",
    "lint:fix": "eslint . --ext .js,.svelte --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^4.14.1",
    "nollup": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^7.0.2",
    "spassr": "^1.1.2"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-yaml": "^2.1.1",
    "@sveltech/routify": "^1.9.9",
    "email-validator": "^2.0.4",
    "postcss-import": "^12.0.1",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^4.2.1"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
